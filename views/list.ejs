<%- include('header'); -%>
<!-- <div></div> -->
<div class="box" id="heading">
    <h1> <%= Typeactivity %></h1>

</div>
<div class="box">
        <% for(let i=0;i<newItems.length;i++ ){%>
            <div class="item"> 
                <input type="checkbox">
                 <p><%= newItems[i] %></p>
            </div>
         <% } %>
         <form action="/" method="post">
            <input type="text" name="newName" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value= <%=Typeactivity %>>+</button>
    
        </form>
</div>
    
<%- include('footer'); -%>
